    "zansbang"
  ],
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/electron": "npm:@ophidian/electron-types@^24.3.1",
    "@types/node": "^22.8.1",
    "@types/sortablejs": "^1.15.8",
    "builtin-modules": "^4.0.0",
    "esbuild": "^0.24.0",
    "i18next": "^23.16.4",
    "monkey-around": "^3.0.0",
    "obsidian": "1.7.2",
    "obsidian-typings": "^2.2.0",
    "sortablejs": "^1.15.3",
    "tslib": "2.8.0",
    "typescript": "5.6.3"
  },
  "dependencies": {
    "ansi-colors": "^4.1.3",
